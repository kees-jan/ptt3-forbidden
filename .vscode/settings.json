{
    "editor.codeLens": true,
    "files.exclude": {
        "**/*.a": true,
        "**/*.d": true,
        "**/*.o": true,
        "**/*~": true,
        "gtest": true,
        "library/test/testForbidden": true,
        "compile_commands.json": true
    },
    "files.associations": {
        "type_traits": "cpp",
        "unique_ptr.h": "cpp",
        "condition_variable": "cpp"
    },
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.intelliSenseMode": "clang-x64",
    "C_Cpp.errorSquiggles": "Enabled",
    "C_Cpp.intelliSenseEngineFallback": "Enabled",
    "C_Cpp.workspaceSymbols": "All",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "googletest.failed",
                "settings": {
                    "foreground": "#f00"
                }
            },
            {
                "scope": "googletest.passed",
                "settings": {
                    "foreground": "#0f0"
                }
            },
            {
                "scope": "googletest.run",
                "settings": {
                    "foreground": "#0f0"
                }
            }
        ]
    },
    "gtest-adapter.supportLocation": false,
    "gtest-adapter.refreshAfterBuild": true,
    "gtest-adapter.runAfterBuild": true,
    "gtest-adapter.debugConfig": "Unittests",
    "gtest-adapter.showRunOutput": false,
}